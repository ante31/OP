//Napi≈°ite funkciju koja stringove, koje korisnik unosi, sprema u vektor stingova,
//svaki string preokrene te sortira vektor po preokrenutim stringovima

#include <iostream>
#include <vector>
#include <algorithm>


using namespace std;

void vector_number(vector <int> &v1, int n)
{
    int num;
    for (int i = 0; i < n; i++)
    {
        cin >> num;
        v1.push_back(num);
    }
}

void vector_interval(vector <int>& v1, int a, int b)
{
    int num;
    cin >> num;
    while ((num >= a) && (num <= b))
    {
        v1.push_back(num);
        cin >> num;
    }
}

void vector_printout(vector <int> v1)
{
    for (int i = 0; i < v1.size(); i++)
        cout << v1[i] << endl;
    cout << endl;
}

void vector_new(vector <int> v1, vector <int> v2, vector <int>& v3)
{
    int condition;
    for (int i = 0; i < v1.size(); i++) {
        condition = 1;
        for (int j = 0; j < v2.size(); j++) {
            if (v1[i] == v2[j]) {
                condition = 0;
                break;
            }
        }
        if (condition)
            v3.push_back(v1[i]);
    }
}

int main()
{
    vector <int> v1, v2, v3;
    vector_number(v1, 3);
    vector_interval(v1, 1, 10);
    vector_number(v2, 3);
    vector_interval(v2, 1, 10);
    vector_new(v1, v2, v3);

    vector_printout(v1);
    vector_printout(v2);
    vector_printout(v3);
}
